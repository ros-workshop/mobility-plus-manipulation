<?xml version="1.0" ?>

<launch>
  <!-- launch the robot -->
  <include file="$(find husky_gazebo)/launch/husky_empty_world.launch"> 
  <arg name="world_name" value="$(find husky_abb_gazebo)/worlds/mobility_manipulation.world"/>"/>
  </include>
  
  <rosparam file="$(find husky_abb_gazebo)/config/joint_state_controller.yaml" command="load" />  
   <node name="joint_state_controller_spawner" pkg="controller_manager" type="controller_manager" args="spawn joint_state_controller" />
	
  <rosparam file="$(find husky_abb_gazebo)/config/controllers.yaml" command="load"/>
   <node name="arm_controller_spawner" pkg="controller_manager" type="controller_manager" args="spawn abb_controller barrett_hand_trajectory_controller" respawn="false" output="screen"/>


  
  <!--Load gps_to_odometry node
  <node pkg="robot_localization" type="navsat_transform_node" name="navsat_transform_node" clear_params="true">
  <rosparam command="load" file="$(find slam_navigation)/config/navsat_transform_node_param.yaml" />
  <remap from="gps/fix" to="navsat/fix"/>
  </node>-->

</launch>
